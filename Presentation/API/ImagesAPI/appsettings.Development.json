{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Console": {
      "FormatterName": "simple",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": true,
        "TimestampFormat": "\\[\\I\\m\\a\\g\\e\\C\\D\\N\\] [yyyy-MM-ddTHH:mm:ss.fffzzz] ",
        "UseUtcTimestamp": false
      }
    }
  }
}
